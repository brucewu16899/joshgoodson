{
    "name": "joshgoodson/joshgoodson",
    "description": "Laravel application powering joshgoodson.com",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "ext-memcached": "^2.2",
        "b3it/laravel-memcached-plus": "^2.1",
        "guzzlehttp/guzzle": "^6.1",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "laravelcollective/html": "^5.1",
        "spatie/laravel-permission": "^1.2",
        "cmgmyr/messenger": "^2.8",
        "lookitsatravis/listify": "^1.1",
        "mmanos/laravel-search": "^1.0",
        "chrisbjr/api-guard": "^2.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "heroku/heroku-buildpack-php": "^83.0",
        "codacy/coverage": "^1.0",
        "laracasts/generators": "^1.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "JoshGoodson\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
